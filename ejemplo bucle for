package bucles;

import java.util.Scanner;

public class bucles {
	public static void main(String[]arg) {
		int a,b;
		Scanner S=new Scanner(System.in);
		
	System.out.println("introduce el numero menor");
	a=S.nextInt();
	System.out.println("introduce el numero mayor");
	b=S.nextInt();
	
	for(int i=a; i<b;i++) {
		System.out.print(i+", ");
	}
	}

}
