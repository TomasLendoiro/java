package intercambioatributos;
import java.util.Scanner;
public class intercambioatributos {

		public static void main(String[] arg) {
			int a,b,aux;
			Scanner s=new Scanner(System.in);
			System.out.println("introduce el valor para el atributo a");
		
		a=s.nextInt();
		
		System.out.println("introduce el valor para el atributo b");
		b=s.nextInt();
		
		aux=a;
		a=b;
		b=aux;
		System.out.println("el valor de a es: "+a+" y el valor del atributo b es:" +b);
		}
}
